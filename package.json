{
  "name": "@clevertree/themed-styler",
  "version": "1.2.9",
  "description": "Themed styler for Relay - Android/iOS Native with JSI support",
  "type": "module",
  "main": "./dist/web/index.js",
  "types": "./dist/web/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/web/index.d.ts",
      "android": "./dist/android/index.android.js",
      "android-ios-native": "./dist/android/index.native.js",
      "import": "./dist/web/index.js",
      "default": "./dist/web/index.js"
    },
    "./android": {
      "types": "./dist/android/index.android.d.ts",
      "import": "./dist/android/index.android.js",
      "default": "./dist/android/index.android.js"
    }
  },
  "files": [
    "dist",
    "android",
    "ios",
    "assets",
    "wasm"
  ],
  "scripts": {
    "build": "tsc && npm run build:wasm",
    "build:wasm": "wasm-pack build --release --target web --features wasm && mkdir -p wasm && cp pkg/themed_styler* wasm/",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "chokidar": "^5.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "ws": "^8.18.3",
    "yaml": "^2.3.0"
  },
  "peerDependencies": {
    "android-ios-native": "*",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "android-ios-native": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "android-ios-native": "*",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
